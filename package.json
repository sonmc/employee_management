{
  "name": "sample-node-typeorm",
  "version": "1.0.0",
  "description": "A simple typeorm configuration for NodeJS and TypeORM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./src/infrastructure/config/ormconfig.ts",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devjayantmalik/sample-node-typeorm.git"
  },
  "keywords": [],
  "homepage": "https://github.com/devjayantmalik/sample-node-typeorm#readme",
  "dependencies": {
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.2",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.x"
  }
}
